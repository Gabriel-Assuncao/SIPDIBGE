Package: SIPDIBGE
Type: Package
Title: Collection of Household Survey Packages Conducted by IBGE
Version: 0.1.0
Authors@R: c(person("Gabriel", "Assuncao", email = "pacotesipd@ibge.gov.br",
                  role = c("aut", "cre")),
             person("Luna", "Hidalgo", email = "pacotesipd@ibge.gov.br",
                  role = c("aut")),
